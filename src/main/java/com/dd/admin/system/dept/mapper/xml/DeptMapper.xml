<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dd.admin.system.dept.mapper.DeptMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.dd.admin.system.dept.entity.Dept">
        <id column="DEPT_ID" property="deptId" />
        <result column="DEPT_NAME" property="deptName" />
        <result column="DEPT_NO" property="deptNo" />
        <result column="PARENT_ID" property="parentId" />
        <result column="PARENT_IDS" property="parentIds" />
        <result column="EXPIRE_DATE" property="expireDate" />
        <result column="REMARK" property="remark" />
        <result column="CREATE_TIME" property="createTime" />
        <result column="CREATE_NAME" property="createName" />
        <result column="CREATE_ID" property="createId" />
        <result column="UPDATE_TIME" property="updateTime" />
        <result column="UPDATE_NAME" property="updateName" />

    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        DEPT_ID, DEPT_NAME, DEPT_NO, PARENT_ID, PARENT_NAME, PARENT_IDS,
        PROVINCE ,CITY ,AREA,
        EXPIRE_DATE, REMARK, CREATE_TIME, CREATE_NAME, CREATE_ID, UPDATE_TIME, UPDATE_NAME
    </sql>

    <select id="selectDeptPage" resultType="com.dd.admin.system.dept.domain.DeptVo">
        select
        *
        from sys_dept where 1 = 1
    </select>

    <select id="selectDeptList" resultType="com.dd.admin.system.dept.domain.DeptVo">
        select
        *
        from sys_dept where 1 = 1
    </select>
</mapper>
